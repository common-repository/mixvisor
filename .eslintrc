{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
  },
  "plugins": [
  ],
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "key-spacing": [
      2,
      {
        "align": "value"
      }
    ],
    "strict": 0,
    "no-multi-spaces": [
      2,
      {
        "exceptions": {
          "VariableDeclarator": true,
          "AssignmentExpression": true
        }
      }
    ]
  }
}
